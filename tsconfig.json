{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "incremental": true,
        "module": "commonjs",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "outDir": "./generators",
        "preserveConstEnums": true,
        "pretty": true,
        "removeComments": true,
        "strictFunctionTypes": false,
        "strictNullChecks": true,
        "strict": true,
        "target": "es6",
        "tsBuildInfoFile": "./reports/tsconfig.tsbuildinfo",
        "lib": ["es6"]
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
